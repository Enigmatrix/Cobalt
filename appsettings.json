{
  "Serilog": {
    "WriteTo": [
      { "Name": "File", "Args": { "path": "Cobalt.txt", "rollingInterval": "Day" } }
    ]
  },
  "EngineLogFilter": "Debug",
  "ConnectionStrings": {
    "QueryContext": "Data Source=main.db"
