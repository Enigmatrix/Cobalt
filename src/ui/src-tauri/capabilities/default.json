{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": ["core:default", "updater:default", "opener:deny-open-path", "opener:deny-reveal-item-in-dir", {
    "identifier": "opener:allow-open-url",
    "allow": [{
      "url": "https://github.com/Enigmatrix/Cobalt/issues"
    }]
  }]
}
